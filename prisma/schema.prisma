datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  description String
  price     Int
  link      String?
  people    Int
  imageUrl  String
  createdAt DateTime @default(now())
}